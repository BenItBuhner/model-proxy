{
  "name": "{{provider_name}}",
  "display_name": "{{display_name}}",
  "enabled": true,

  "api_keys": {
    "env_var_pattern": "{PROVIDER}_API_KEY",
    "env_var_patterns": [
      "{PROVIDER}_API_KEY",
      "{PROVIDER}_API_KEY_{INDEX}"
    ],
    "description": "Environment variable patterns for API keys. {PROVIDER} = '{{provider_name_upper}}', {INDEX} = 1,2,3..."
  },

  "endpoints": {
    "base_url": "{{base_url}}",
    "completions": "/v1beta/models/{{model}}:generateContent",
    "streaming": "/v1beta/models/{{model}}:streamGenerateContent",
    "compatible_format": "gemini"
  },

  "authentication": {
    "type": "bearer",
    "header_name": "x-goog-api-key",
    "header_format": "{api_key}"
  },

  "request_config": {
    "timeout_seconds": 60,
    "max_retries": 3,
    "retry_on_status": [429, 500, 502, 503, 504],
    "default_parameters": {
      "temperature": 0.7,
      "topP": 0.8,
      "maxOutputTokens": 1024
    }
  },

  "rate_limiting": {
    "enabled": false,
    "requests_per_minute": null,
    "tokens_per_minute": null
  },

  "proxy_support": {
    "enabled": false,
    "base_url_override": null,
    "description": "If enabled, use base_url_override instead of base_url"
  }
}
