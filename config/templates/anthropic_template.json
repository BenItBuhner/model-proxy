{
  "name": "{{provider_name}}",
  "display_name": "{{display_name}}",
  "enabled": true,

  "api_keys": {
    "env_var_pattern": "{PROVIDER}_API_KEY",
    "env_var_patterns": [
      "{PROVIDER}_API_KEY",
      "{PROVIDER}_API_KEY_{INDEX}"
    ],
    "description": "Environment variable patterns for API keys. {PROVIDER} = '{{provider_name_upper}}', {INDEX} = 1,2,3..."
  },

  "endpoints": {
    "base_url": "{{base_url}}",
    "completions": "/v1/messages",
    "streaming": "/v1/messages",
    "compatible_format": "anthropic"
  },

  "authentication": {
    "type": "x-api-key",
    "header_name": "x-api-key",
    "header_format": "{api_key}",
    "additional_headers": {
      "anthropic-version": "2023-06-01"
    }
  },

  "request_config": {
    "timeout_seconds": 60,
    "max_retries": 3,
    "retry_on_status": [429, 500, 502, 503, 504],
    "default_parameters": {
      "temperature": null,
      "top_p": null,
      "max_tokens": 4096
    },
    "required_parameters": ["max_tokens"]
  },

  "rate_limiting": {
    "enabled": false,
    "requests_per_minute": null,
    "tokens_per_minute": null
  },

  "proxy_support": {
    "enabled": false,
    "base_url_override": null,
    "description": "If enabled, use base_url_override instead of base_url (for proxies)"
  }
}
