{
  "name": "nahcrof",
  "display_name": "Nahcrof AI",
  "enabled": true,

  "api_keys": {
    "env_var_patterns": [
      "NAHCROF_API_KEY",
      "NAHCROF_API_KEY_{INDEX}"
    ],
    "description": "Environment variable patterns for API keys. {INDEX} = 1,2,3..."
  },

  "endpoints": {
    "base_url": "https://ai.nahcrof.com",
    "completions": "/v2/chat/completions",
    "streaming": "/v2/chat/completions",
    "compatible_format": "openai"
  },

  "authentication": {
    "type": "bearer",
    "header_name": "Authorization",
    "header_format": "Bearer {api_key}"
  },

  "request_config": {
    "timeout_seconds": 180,
    "max_retries": 50,
    "retry_on_status": [429, 500, 502, 503, 504],
    "default_parameters": {
      "temperature": 1.0,
      "top_p": 1.0,
      "n": 1
    }
  },

  "rate_limiting": {
    "enabled": false,
    "requests_per_minute": null,
    "tokens_per_minute": null,
    "cooldown_seconds": 180
  },

  "models": {},

  "error_handling": {
    "400": {
      "action": "fallback_no_cooldown"
    },
    "500": {
      "action": "fallback_no_cooldown"
    },
    "401": {
      "action": "global_key_failure"
    },
    "403": {
      "action": "global_key_failure"
    },
    "429": {
      "action": "model_key_failure"
    }
  },

  "proxy_support": {
    "enabled": false,
    "base_url_override": null,
    "description": "If enabled, use base_url_override instead of base_url (for proxies)"
  }
}
