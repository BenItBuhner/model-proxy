{
  "name": "gemini",
  "display_name": "Gemini",
  "enabled": true,

  "api_keys": {
    "env_var_patterns": ["GEMINI_API_KEY", "GEMINI_API_KEY_{INDEX}"],
    "description": "Environment variable patterns for API keys. {INDEX} = 1,2,3..."
  },

  "endpoints": {
    "base_url": "https://generativelanguage.googleapis.com/v1beta",
    "completions": "/models/{model}:generateContent",
    "streaming": "/models/{model}:streamGenerateContent",
    "compatible_format": "native"
  },

  "authentication": {
    "type": "api_key",
    "header_name": "x-goog-api-key",
    "header_format": "{api_key}",
    "note": "Native SDK handles authentication automatically"
  },

  "request_config": {
    "timeout_seconds": 60,
    "max_retries": 3,
    "retry_on_status": [429, 500, 502, 503, 504],
    "default_parameters": {
      "temperature": 1.0,
      "top_p": 1.0
    }
  },

  "rate_limiting": {
    "enabled": false,
    "requests_per_minute": null,
    "tokens_per_minute": null
  },

  "proxy_support": {
    "enabled": false,
    "base_url_override": null,
    "description": "If enabled, use base_url_override instead of base_url (for proxies)"
  },

  "provider_notes": {
    "sdk": "google-genai",
    "description": "Uses the native Google GenAI Python SDK for better compatibility with tools, streaming, and multimodal content"
  }
}
